@model List<OLESClass.Test>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>CreateLobby</title>
</head>
<body>
<div>
    @Html.BeginForm("CreateLobby","Lobby",FormMethod.Post)
    {
    <select name="testName">
        @foreach (var item in Model)
        {
            <option>@item.Name</option>
        }
    </select>
    <input name="QuestionCount" type="number" class="form-control" value="1" max="5" min ="1"/>
    <input type="submit" value="Select" class="btn btn-success" />

  }
</div>
</body>
</html>
